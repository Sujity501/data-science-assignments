    "def create_ascii_dictionary():\n",
    "    ascii_dict = {}\n",
    "    for letter in range(ord('a'), ord('z')+1):\n",
    "        ascii_dict[chr(letter)] = letter\n",
    "    return ascii_dict\n",
    "\n",
    "ascii_dictionary = create_ascii_dictionary()\n",
    "\n",
    "for key, value in ascii_dictionary.items():\n",
    "    print(f\"{key}: {value}\")\n"
